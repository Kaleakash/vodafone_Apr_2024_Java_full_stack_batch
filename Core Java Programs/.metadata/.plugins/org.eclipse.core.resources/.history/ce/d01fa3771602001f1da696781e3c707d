package abstractdemo;
abstract class Bike {
	abstract void speed();
	void mailage() {
		System.out.println("50km/lt");
	}
}
class Honda extends Bike {
	@Override
	void mailage() {
		System.out.println("70km/lt");
	}
	@Override
	void speed() {
		System.out.println("50km/hr");
	}
	void color() {
		System.out.println("Black");
	}
}
class Pulsar extends Bike {
	void color() {
		System.out.println("Gray");
	}
	@Override
	void speed() {
		System.out.println("90km/hr");
	}
}
class Tvs extends Bike {
	void color() {
		System.out.println("Gray");
	}
	@Override
	void speed() {
		System.out.println("90km/hr");
	}
}
public class AbstractExample {
	public static void main(String[] args) {
	Honda hh  = new Honda();		hh.color();    hh.speed();   hh.mailage();
	Pulsar pu  = new Pulsar();		pu.color();    pu.speed();   pu.mailage();
	Tvs tv = new Tvs();				tv.color();    tv.speed();   tv.mailage();
	}
}
